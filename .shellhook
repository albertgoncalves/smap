#!/usr/bin/env bash

export WD=$PWD

if [ ! -d "$WD/out" ]; then
    mkdir "$WD/out"
fi

gg () {
    game=$(./scripts/game.sh "$1") || return
    plot=$(./scripts/plot.py "$game") || return
    feh "$plot"
}

jqc () {
    jq . "$1" -C | less -R
}

export -f gg
export -f jqc
alias flake8="flake8 --ignore=E722,W503"
